{
  "name": "nats-firehose",
  "type": "module",
  "version": "0.0.0",
  "description": "A sync experiment with AT Proto and NATS",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "kv": "pnpm --filter record-kv kv",
    "ingest": "pnpm --filter record-kv ingest",
    "like-indexer": "pnpm --filter example like-indexer",
    "get-likes-by": "pnpm --filter example get-likes-by",
    "nats-reset": "nats stream del ingest && nats kv del cursor && nats kv del record"
  },
  "keywords": [],
  "author": "Devin Ivy <devin@blueskyweb.xyz>",
  "license": "MIT",
  "engines": {
    "node": ">=24.3.0"
  },
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@types/node": "^24.0.10",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.35.1"
  }
}
